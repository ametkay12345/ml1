euclidian_dist <- function(u, v)
{
  return (sqrt(sum((u - v)^2)))
}


sort_data_by_dist <- function(data, u)
{
  row <- dim(data)[1]
  col <- dim(data)[2] - 1
  
  distances <- matrix(NA, 1, 2)
  for(i in 1:row)
  {
    distances[i, ] <- c(i, euclidian_dist(data[i, 1:col], u))
  }
  ans <- data[order(distances[, 2]), ]
  print(ans)
  return (ans)
}


